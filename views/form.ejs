<section id="main-content">
 <section class="wrapper">
    <div v-if="post.id!=null && post.id!=''">
    <h2>Update Post</h2>
    </div>
    <div v-else>
      <h2>Create Post</h2>
      </div>
    <!-- BASIC FORM ELELEMNTS -->
    <div class="row mt">
      <div class="col-lg-12">
        <div class="form-panel">
          <form class="form-horizontal style-form" method="post" enctype="multipart/form-data">
            <div class="form-group">
              <label class="col-sm-2 col-sm-2 control-label">Title</label>
              <div class="col-sm-10">
                <input type="text" class="form-control" placeholder="Enter title of the post" v-model="post.title">
              </div>
            </div>
        
            <div class="form-group">
              <label class="col-sm-2 col-sm-2 control-label">Select Category</label>
              <div class="col-sm-10">
                <select class="form-control" v-model="post.category">
                    <option value='' disabled selected hidden>Select category</option>
                    <option v-for="category in categories" :value="category">{{category}}</option>
                    
                </select>
              </div>
            </div>
            <div class="form-group">
              <label class="control-label col-md-3">Featured Image</label>
              <div class="col-md-9">
                <div class="fileupload fileupload-new" data-provides="fileupload">
                  <div class="fileupload-new thumbnail" style="width: 200px; height: 150px;">
                    <img src="http://www.placehold.it/200x150/EFEFEF/AAAAAA&text=no+image" alt="" />
                  </div>
                  <div class="fileupload-preview fileupload-exists thumbnail" style="max-width: 200px; max-height: 150px; line-height: 20px;"></div>
                  <div>
                    <span class="btn btn-theme02 btn-file">
                      <span class="fileupload-new"><i class="fa fa-paperclip"></i> Select image</span>
                    <span class="fileupload-exists"><i class="fa fa-undo"></i> Change</span>
                    <input type="file" class="default" />
                    </span>
                    
                  </div>
                </div>
              
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 col-sm-2 control-label">Meta Description</label>
              <div class="col-sm-10">
                <textarea  class="form-control" rows="3" v-model="post.metaDescription" placeholder="Enter meta description between 155-160"></textarea>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 col-sm-2 control-label">Excerpt</label>
              <div class="col-sm-10">
                <textarea  class="form-control" rows="3" v-model="post.excerpt" placeholder="Enter excerpt within 300 characters"></textarea>
              </div>
            </div>
            <div class="row form-group">
              <label class="col-sm-2 col-sm-2 control-label">Tags</label>
              <div class="col-sm-10">
                <multiselect
                  v-model="post.tags"
                  placeholder="Add tags here"
                  :options="post.tags"
                  :multiple="true"
                  :taggable="true"
                  @tag="addTags"
                ></multiselect>
              </div>
            </div>
            <div class="form-group">
              <label class="col-sm-2 col-sm-2 control-label">Description</label>
              <div class="col-sm-10">
                <ckeditor :editor="editor" v-model="post.description" :config="editorConfig"></ckeditor>
              </div>
            </div>
            <div class="form-group">
            <div class="col-sm-10">
            <button type="button" class="btn btn-primary btn-sm" @click="addPost">
              <i class="fa fa-dot-circle-o"></i> Submit
            </button>
            <button type="reset" class="btn btn-danger btn-sm">
              <i class="fa fa-ban"></i> Reset
            </button>
          </div>
        </div>
          </form>
        </div>
      </div>
      <!-- col-lg-12-->
    </div>
